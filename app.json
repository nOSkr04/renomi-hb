{
  "expo": {
    "name": "nomi-hbs",
    "slug": "nomi-hbs",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/ios-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#313C59"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIViewControllerBasedStatusBarAppearance": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/android-logo.png",
        "backgroundColor": "#313C59"
      }
    },
    "plugins": [
      "expo-font",
      "expo-video"
    ]
  }
}
